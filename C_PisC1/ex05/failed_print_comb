#include <unistd.h>

void	ft_print_comb(void)
{
	int i =	 0;
	int j =	 1;
	int k =	 2;


	while (i < 10)
	{
		while (j < 10)
		{
			while (k < 10)
			{
				if (i != j && j != k)
				{
					char a = i + '0';
					char b = j + '0';
					char c = k + '0';
					write(1, &a, 1);
					write(1, &b, 1);
					write(1, &c, 1);
					write(1, ", ", 2);
				}
				k = k + 1;
			}
			j = j + 1;
		}
		i = i + 1;
		j = i + 1;
		k = j + 1;
	}
}

int	main(void)
{
	ft_print_comb();
	return(0);
}
